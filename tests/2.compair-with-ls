
set -e

t=`lf -1 / | sort`
e=`ls -1 / | sort`

if [ "$t" != "$e" ]; then
	echo "FAIL: expecting: $e, got: $t"
	exit 1
fi

t=`lf -1 /tmp | sort`
e=`ls -1 /tmp | sort`

if [ "$t" != "$e" ]; then
	echo "FAIL: expecting: $e, got: $t"
	exit 1
fi

t=`lf -1 /dev | sort`
e=`ls -1 /dev | sort`

if [ "$t" != "$e" ]; then
	echo "FAIL: expecting: $e, got: $t"
	exit 1
fi
